{
  "name": "bullmq-task-scheduler",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig.root.json",
    "build:prod": "NODE_ENV=production npm run build",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "dev": "NODE_ENV=development nodemon --exec ts-node index.ts",
    "worker": "node dist/worker.js",
    "worker:dev": "NODE_ENV=development nodemon --exec ts-node worker.ts",
    "worker:prod": "NODE_ENV=production node dist/worker.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "A robust task scheduler system built with TypeScript, BullMQ, and Redis",
  "dependencies": {
    "@bull-board/api": "^6.9.2",
    "@bull-board/express": "^6.9.2",
    "bullmq": "^5.49.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
